#include "ZA.hpp"

ZoneAgricole::ZoneAgricole(int SurfaceConstructible, int numero, string proprietaire, Polygone<int, float> forme, float surface, std::string culture)
    : Parcelle(numero, proprietaire, forme, false, false), Constructible(SurfaceConstructible), ZoneAgricole(culture),  {
    // Initialisation spécifique à ZoneNaturelle si nécessaire
}

std::string ZoneAgricole::setculture(std::string cult) const
{
  culture = cult;
}
//constructeur:
ZoneAgricole::ZoneAgricole(int SurfaceConstructible, int numero, std::string proprietaire, Polygone<int, float> forme, float surface);


std::ostream& operator<<(std::ostream& os, const ZoneAgricole& za) {

  os << "Parcelle n°" << za.getNumero() << std::endl;
  //os << "Type : " << "ZA" << endl;
  //os << "Polygone: " << za.getForme() << endl;
  os << "Proprietaire : " << za.getProprietaire() << std::endl;
  os << "Surface : " << za.getSurface() <<std::endl;
  os << "% Constructible : " << za.getpConstructible() << std::endl;
  //os << "Culture : " << ZoneAgricole.culture << std::endl;

    return os;
}